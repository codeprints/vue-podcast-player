{
  "private": false,
  "name": "xns-audio-player-simple",
  "version": "2.0.2",
  "description": "a simple yet powerful music player for vuejs based on xns-audio-player",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vue serve src/XnsAudioPlayerSimple.vue",
    "build": "npx bili --format cjs --format umd --module-name xns-player-umd --bundle-node-modules --minify --no-map",
    "build:css": "npx tailwind build src/style.css -o dist/xns-audio-player-simple.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinnks/xns-audio-player-simple.git"
  },
  "keywords": [
    "audio player",
    "audio",
    "player",
    "xinnks",
    "vue",
    "vuejs"
  ],
  "author": "James Sinkala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xinnks/xns-audio-player-simple/issues"
  },
  "homepage": "https://github.com/xinnks/xns-audio-player-simple#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "bili": "^4.10.1",
    "cssnano": "^4.1.10",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-vue": "^4.2.0",
    "tailwindcss": "^1.9.6",
    "vue": "^2.6.12",
    "vue-loader": "^14.2.4",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "vue-ionicons": "^3.0.5"
  },
  "eslintConfig": {
    "rules": {
      "no-console": "off"
    }
  }
}
